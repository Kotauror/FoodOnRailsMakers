<%= render 'header' %>
<div class="notice">
  <%= notice %>
</div>

<%= form_for(@restaurant) do |form| %>
  <p>
    <%= form.label :name %><br/>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :description %><br/>
    <%= form.text_area :description %>
  </p>

  <%= form.submit %>
<% end %>
<%= render 'footer' %>
